include ./_components.jade

mixin card (title, text, link)
	.card&attributes(attributes)
		.card--title!= title
		.card--text
			each p in text
				p!= p

		.card--link ...#[a(href=link) show more]

mixin table--row (pageID, address, permitID, units, issued, approvalTime)
	.table--row(data-page=pageID)&attributes(attributes)
		.table--td!= address
		.table--td!= permitID
		.table--td!= units
		.table--td!= issued
		.table--td!= approvalTime
		.table--td: a(href='https://dbiweb.sfgov.org/dbipts/default.aspx?page=Permit&PermitNumber=' + permitID) Link

mixin box--foot
	.box--foot
		svg(viewBox="0 0 10 13" width="10" height="13")
			path(d="M7.4,3.4H10L6.8,0v2.9C6.8,3.2,7.1,3.4,7.4,3.4z")
			path(d="M7.4,3.9c-0.6,0-1.1-0.4-1.1-1V0H1.1C0.5,0,0,0.4,0,1V12c0,0.5,0.5,1,1.1,1h4.2h2.6h1.1c0.6,0,1.1-0.4,1.1-1 V3.9H7.4z")

		block

doctype html
html
	+head (meta)
		link(href="https://api.tiles.mapbox.com/mapbox-gl-js/v0.45.0/mapbox-gl.css" rel="stylesheet")

	body
		.page: .container
			header.header
				.header--logo
				.header--box
					.header--title SFHAC Dashboard
					.header--text San Francisco Housing in Numbers

					a(href=links.sfhac).header--sfhac

			.page--in
				.page--side: .page--sticky
					.intro
						.intro--logo
						.intro--box
							.intro--title SFHAC Dashboard
							.intro--text San Francisco Housing in Numbers

					.box: nav.nav: ul.nav--list
						li.is-active: a(href="#" data-box-nav="1") Housing forecast
						li: a(href="#" data-box-nav="2") Growth capacity
						li: a(href="#" data-box-nav="3") Building permits
						li: a(href="#" data-box-nav="4") Voter guide

					.box: .share
