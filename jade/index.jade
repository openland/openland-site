include ./_components.jade

doctype html
html
	head
		meta(charset="utf-8")

		title Openland — A better way to make land acquisitions
		meta(name="description" content="End-to-end service to accelerate your land acquisition process")

		meta(name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, user-scalable=no")
		meta(name="format-detection" content="telephone=no")

		meta(property="og:title" content="Openland — A better way to make land acquisitions")
		meta(property="og:url" content="https://openland.com/")
		meta(property="og:description" content="End-to-end service to accelerate your land acquisition process")
		meta(property="og:image" content="https://openland.com/img/og.jpg")

		link(rel="shortcut icon" href="favicon.ico")
		link(rel="icon" href="favicon.png")

		link(rel="stylesheet" type="text/css" href="css/style.css")

	body
		.page
			header.header(data-animate="load"): .container: .header--in
				a(href="#").header--logo: img(src="img/logotype.svg" alt="")
				a(href="#").header--open

				nav.header--nav: ul.header--list
					li.header--item.is-active: span Builders
					li.header--item: a(href="#ownersPopup").js-modals-open Owners
					li.header--item: a(href="#policymakersPopup").js-modals-open Policymakers

				.header--btn: a(href=signIn).btn Sign in

			.container
				.intro
					div(data-animate="load" data-animate-delay="200")
						.intro--title A better way to make land acquisitions
						.intro--text Openland is an end-to-end service to organize your dealflow and get connected to more opportunities
						.intro--form: +form ('Request a demo')

					.intro--illustration(data-animate="load" data-animate-delay="500")
						+intro--row (['Publicly owned land', 'Single story retail', 'Parking lot', 'Backyard', 'Recently-upzoned area'])

						.intro--image: img(src="img/buildings.png" srcset="img/buildings@2x.png 2x" alt="")

				.features(data-animate="scroll")
					.features--text Openland is created for #[span ambitious builders] who want to #[span buy more, faster]#[br.hidden-xs] using #[span one integrated system]
					.features--lines #[.features--line]#[.features--line]#[.features--line]#[.features--line]
					.features--grid
						+features--item ('land', 'Land prospecting', ['Filter-based search', 'Integrated data sources', 'Intake forms for owners', 'Notifications'])
						+features--item ('calc', 'Evaluations', ['Zoning requirements', 'Capacity analysis', 'Financial feasibility', 'Deal checklist'])
						+features--item ('docs', 'Documents', ['Deal summary', 'Offers and contracts', 'Government forms', 'Financing'])
						+features--item ('arrows', 'Dealflow', ['Deals by stage', 'Contact management', 'Tasks and action items', 'Dashboard and reporting'])

				.available(data-animate="scroll")
					.available--title Data integrations
					.available--in: .available--box #[span US Census]#[span Permit history]#[span Tax assestments]#[span Zoning]#[span Satellite maps]#[span Street view]#[span Transit maps]

				.map(data-animate="scroll")
					.map--title Openland in action: #[span San Francisco Land Inventory]
					.map--text All 154,449 parcels in the city, organized by size, location, zoning, current use, tax assessment, and permit history.
					.map--wrap
						.map--label.visible-xs Opportunity lists
						.map--tabs
